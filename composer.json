{
    "name": "human018/nova-slide-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Human018\\NovaSlideField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Human018\\NovaSlideField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
